<?xml version="1.0" encoding="UTF-8"?>
<elementDefinition xmlns="http://www.xframium.org/pageRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/pageRegistry http://www.xframium.org/xsd/pageManagement/pageRegistry.xsd">
<site name="MMT">
  <page name="LoginPage" class="package pages.LoginPage">
  
    <element name="icon_LoginM" value="//a[@class='login']" descriptor="XPATH"/>
    <element name="txt_usernameMMT" value="//input[@id='email']" descriptor="XPATH"/>
    <element name="txt_passwordMMT" value="//input[@id='passwd']" descriptor="XPATH"/>
    <element name="btn_signinMMT" value="//button[@id='SubmitLogin']" descriptor="XPATH"/>
    <element name="btn_logo" value="//img[@class='logo img-responsive']" descriptor="XPATH"/>
    <element name="btn_signoffFP" value="//a[@class='logout']" descriptor="XPATH"/>
  </page>
  
  
  <page name="MenuItems" class="package pages.MenuItems">
    <element name="btn_womenMenu" value=" //ul[@class='sf-menu clearfix menu-content sf-js-enabled sf-arrows']/li[1]" descriptor="XPATH"/>
 	<element name="btn_dressesMenu" value=" //ul[@class='sf-menu clearfix menu-content sf-js-enabled sf-arrows']/li[2]" descriptor="XPATH"/>
 	<element name="btn_tshirtsMenu" value=" //ul[@class='sf-menu clearfix menu-content sf-js-enabled sf-arrows']/li[3]" descriptor="XPATH"/>
  </page>
  
  </site>
</elementDefinition>